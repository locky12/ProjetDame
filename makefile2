	# 	gcc -c -lncurses src/login.c

src/affichage.o: src/affichage.c src/affichage.h src/struct.h
	gcc -c src/affichage.c

src/deplacement.o: src/deplacement.c src/deplacement.h src/struct.h
	gcc -c src/deplacement.c
