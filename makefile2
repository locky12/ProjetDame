



server: login.o serveurDame.o SQLite.o src/deplacement.o src/affichage.o
	gcc -o server serveurDame.o login.o SQLite.o src/deplacement.o src/affichage.o -lpthread -lsqlite3

login.o: src/login.c src/login.h
	gcc -c -lncurses src/login.c

serveurDame.o: src/serveurDame.c src/serveurDame.h src/struct.h
	gcc -c src/serveurDame.c -lpthread

